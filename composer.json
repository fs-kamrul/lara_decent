{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "arcanedev/log-viewer": "^8.3",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "composer/semver": "^3.2",
        "ezyang/htmlpurifier": "v4.13",
        "fruitcake/laravel-cors": "^2.0",
        "google/analytics-data": "^0.11.0",
        "google/apiclient": "^2.13",
        "google/apiclient-services": "^0.284.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "khaled.alshamaa/ar-php": "^6.3",
        "kris/laravel-form-builder": "^1.53",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "maatwebsite/excel": "^3.1",
        "mews/purifier": "^3.3",
        "mpdf/mpdf": "^8.2",
        "nwidart/laravel-menus": "^7.0",
        "nwidart/laravel-modules": "^8.2",
        "spatie/laravel-backup": "^6.16",
        "stripe/stripe-php": "^10.13",
        "symfony/cache": "^6.2",
        "tightenco/ziggy": "^1.6",
        "twig/twig": "^3.0",
        "xantios/mimey": "^2.2",
        "yajra/laravel-datatables-buttons": "^4",
        "yajra/laravel-datatables-html": "^4",
        "yajra/laravel-datatables-oracle": "^9.19"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
